{
  "name": "puppeteer-scripts",
  "version": "1.0.0",
  "description": "puppeteer scripts to get image and bundle metrics for websites",
  "main": "index.js",
  "scripts": {
    "test": "ENV=test jest --coverage --config jest.config.json",
    "test:watch": "ENV=test jest --watch --notify --notifyMode=change --coverage --config jest.config.json",
    "dev": "ENV=dev node ./src/index.js",
    "start": "node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baneDD/puppeteer-scripts.git"
  },
  "author": "Bane Debeljevic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baneDD/puppeteer-scripts/issues"
  },
  "homepage": "https://github.com/baneDD/puppeteer-scripts#readme",
  "dependencies": {
    "cron": "^1.7.0",
    "influx": "^5.0.7",
    "puppeteer": "^1.14.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "codecov": "^3.3.0",
    "jest": "^24.7.1"
  }
}
